#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = Bookviewer2.exe Bookviewer2.CHS
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

Bookviewer2.exe: Bookviewer2.dpr
  $(DCC)

Bookviewer2.CHS: CHS\Bookviewer2.dpr
  $(DCC)


